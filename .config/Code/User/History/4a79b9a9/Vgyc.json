    "drizzle-kit": "^0.22.7",
    "eslint-plugin-security": "^3.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "bcrypt": "^5.1.1",
